<!DOCTYPE html>
<html lang="en">
<head>
    <title>ConUshop | Home </title>
    <% include ../components/header.ejs %>

</head>
<body>
<!-- for bootstrap working -->

<script type="text/javascript" src="/js/bootstrap-3.1.1.min.js"></script>
<script type="text/javascript" src="/js/main.js"></script>
<!-- //for bootstrap working -->
<!-- navigator -->
<% include ../components/navigator.ejs %>
<% include ../components/clientNav.ejs %>
<!-- //navigator -->
<div class="client_content">



    <div>
            <% if (typeof data=='undefined') { %>
                <div>
                    <br>
                    <h2>Welcome to ConUshop!</h2><br/>
                    <img src="\images\concordia.jpg" />
                </div>
            <%}%>

            <% if (typeof data!='undefined' && data.table=='desktop') { %>

            <h1>Desktop</h1>
            <br>
            <table contenteditable="true" id="prodTable">
                <tr contenteditable="false">
                    <td>Model</td>
                    <td>Brand</td>
                    <td>Processor</td>
                    <td>Ram</td>
                    <td>Storage</td>
                    <td>Cores</td>
                    <td>Dimensions</td>
                    <td>Weight</td>
                    <td>Price</td>

                </tr>
                <% for(var i = 0; i < data.length; i++) { %>
                <tr>
                    <td><%= data[i].model %></td>
                    <td><%= data[i].brand %></td>
                    <td><%= data[i].processor %></td>
                    <td><%= data[i].ram %></td>
                    <td><%= data[i].storage %></td>
                    <td><%= data[i].cores %></td>
                    <td><%= data[i].dimensions %></td>
                    <td><%= data[i].weight %></td>
                    <td><%= data[i].price %></td>
                </tr>
                <% } %>
            </table>
            <% } %>
        <% if (typeof data!='undefined' && data.table=='laptop') { %>
        <h1>Laptop</h1>
        <br>
        <table contenteditable="true" id="prodTable">
            <tr contenteditable="false">
                <td>Model</td>
                <td>Brand</td>
                <td>Processor</td>
                <td>Ram</td>
                <td>Storage</td>
                <td>Cores</td>
                <td>Os</td>
                <td>Battery</td>
                <td>Camera</td>
                <td>Touch</td>
                <td>Dimensions</td>
                <td>Weight</td>
                <td>Price</td>

            </tr>
            <% for(var i = 0; i < data.length; i++) { %>
            <tr>
                <td><%= data[i].model %></td>
                <td><%= data[i].brand %></td>
                <td><%= data[i].processor %></td>
                <td><%= data[i].ram %></td>
                <td><%= data[i].storage %></td>
                <td><%= data[i].cores %></td>
                <td><%= data[i].os %></td>
                <td><%= data[i].battery %></td>
                <td><%= data[i].camera %></td>
                <td><%= data[i].touch %></td>
                <td><%= data[i].dimensions %></td>
                <td><%= data[i].weight %></td>
                <td><%= data[i].price %></td>
            </tr>
            <% } %>
        </table>
        <% } %>
        <% if (typeof data!='undefined' && data.table=='monitor') { %>
        <h1>Monitor</h1>
        <br>
        <table contenteditable="true" id="prodTable">
            <tr contenteditable="false">
                <td>Model</td>
                <td>Brand</td>
                <td>Size</td>
                <td>Weight</td>
                <td>Price</td>

            </tr>
            <% for(var i = 0; i < data.length; i++) { %>
            <tr>
                <td><%= data[i].model %></td>
                <td><%= data[i].brand %></td>
                <td><%= data[i].size %></td>
                <td><%= data[i].weight %></td>
                <td><%= data[i].price %></td>
            </tr>
            <% } %>
        </table>
        <% } %>
        <% if (typeof data!='undefined' && data.table=='tablet') { %>
        <h1>Tablet</h1>
        <br>
        <table contenteditable="true" id="prodTable">
            <tr contenteditable="false">
                <td>Model</td>
                <td>Brand</td>
                <td>Display</td>
                <td>Processor</td>
                <td>Ram</td>
                <td>Storage</td>
                <td>Cores</td>
                <td>Os</td>
                <td>Battery</td>
                <td>Camera</td>
                <td>Dimensions</td>
                <td>Weight</td>
                <td>Price</td>

            </tr>
            <% for(var i = 0; i < data.length; i++) { %>


            <tr>
                <td><%= data[i].model %></td>
                <td><%= data[i].brand %></td>
                <td><%= data[i].display %></td>
                <td><%= data[i].processor %></td>
                <td><%= data[i].ram %></td>
                <td><%= data[i].storage %></td>
                <td><%= data[i].cores %></td>
                <td><%= data[i].os %></td>
                <td><%= data[i].battery %></td>
                <td><%= data[i].camera %></td>
                <td><%= data[i].dimensions %></td>
                <td><%= data[i].weight %></td>
                <td><%= data[i].price %></td>
            </tr>
            <% } %>
        </table>
        <% } %>

    </div>
</div>
<!--%= jsonData %-->
<!-- footer -->
<% include ../components/footer.ejs %>
<!-- //footer -->

</body>
</html>
